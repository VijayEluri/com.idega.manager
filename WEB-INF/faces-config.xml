<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>


	<managed-bean>
		<managed-bean-name>LoginManager</managed-bean-name>
		<managed-bean-class>com.idega.manager.bean.LoginManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>InstallOrUpdateManager</managed-bean-name>
		<managed-bean-class>com.idega.manager.bean.InstallOrUpdateManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>UpdateListManager</managed-bean-name>
		<managed-bean-class>com.idega.manager.bean.UpdateListManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ModuleManager</managed-bean-name>
		<managed-bean-class>com.idega.manager.bean.ModuleManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>InstallManager</managed-bean-name>
		<managed-bean-class>com.idega.manager.bean.InstallManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>InstallListManager</managed-bean-name>
		<managed-bean-class>com.idega.manager.bean.InstallListManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>InstallNewModuleListManager</managed-bean-name>
		<managed-bean-class>com.idega.manager.bean.InstallNewModuleListManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
  		<from-view-id>/idegaweb/bundles/com.idega.manager.bundle/jsp/LoginManager.jsp</from-view-id>
  		<navigation-case>
    	<from-outcome>next</from-outcome>
    	<to-view-id>/workspace/manager/Install/InstallUpdate/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>cancel</from-outcome>
    	<to-view-id>/workspace/</to-view-id>
  		</navigation-case>
	</navigation-rule>
	<navigation-rule>
  		<from-view-id>/idegaweb/bundles/com.idega.manager.bundle/jsp/InstallOrUpdateManager.jsp</from-view-id>
  		<navigation-case>
    	<from-outcome>updateModules</from-outcome>
    	<to-view-id>/workspace/manager/Install/Update/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>installNewModules</from-outcome>
    	<to-view-id>/workspace/manager/Install/NewModule/</to-view-id>
  		</navigation-case>
 		<navigation-case>
    	<from-outcome>cancel</from-outcome>
    	<to-view-id>/workspace/</to-view-id>
  		</navigation-case>
	</navigation-rule>
	<navigation-rule>
  		<from-view-id>/idegaweb/bundles/com.idega.manager.bundle/jsp/UpdateListManager.jsp</from-view-id>
  		<navigation-case>
    	<from-outcome>next</from-outcome>
    	<to-view-id>/workspace/manager/Install/Commit/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>back</from-outcome>
    	<to-view-id>/workspace/manager/Install/InstallUpdate/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>cancel</from-outcome>
    	<to-view-id>/workspace/</to-view-id>
  		</navigation-case>
	</navigation-rule>
	<navigation-rule>
  		<from-view-id>/idegaweb/bundles/com.idega.manager.bundle/jsp/ModuleManager.jsp</from-view-id>
  		<navigation-case>
    	<from-outcome>backUpdate</from-outcome>
    	<to-view-id>/workspace/manager/Install/Update/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>backInstall</from-outcome>
    	<to-view-id>/workspace/manager/Install/NewModuleVersion/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>cancel</from-outcome>
    	<to-view-id>/workspace/</to-view-id>
  		</navigation-case>
	</navigation-rule>
	<navigation-rule>
  		<from-view-id>/idegaweb/bundles/com.idega.manager.bundle/jsp/InstallListManager.jsp</from-view-id>
  		<navigation-case>
    	<from-outcome>next</from-outcome>
    	<to-view-id>/workspace/manager/Install/NewModuleVersion/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>back</from-outcome>
    	<to-view-id>/workspace/manager/Install/InstallUpdate/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>cancel</from-outcome>
    	<to-view-id>/workspace/</to-view-id>
  		</navigation-case>
	</navigation-rule>
	<navigation-rule>
  		<from-view-id>/idegaweb/bundles/com.idega.manager.bundle/jsp/InstallNewModuleListManager.jsp</from-view-id>
  		<navigation-case>
    	<from-outcome>next</from-outcome>
    	<to-view-id>/workspace/manager/Install/Commit/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>back</from-outcome>
    	<to-view-id>/workspace/manager/Install/NewModule/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>cancel</from-outcome>
    	<to-view-id>/workspace/</to-view-id>
  		</navigation-case>
	</navigation-rule>
</faces-config>
