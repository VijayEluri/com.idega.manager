<?xml version="1.0"?>


<faces-config
	xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
version="1.2">


	<managed-bean>
		<managed-bean-name>LoginManager</managed-bean-name>
		<managed-bean-class>com.idega.manager.bean.LoginManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>InstallOrUpdateManager</managed-bean-name>
		<managed-bean-class>com.idega.manager.bean.InstallOrUpdateManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>UpdateListManager</managed-bean-name>
		<managed-bean-class>com.idega.manager.bean.UpdateListManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ModuleManager</managed-bean-name>
		<managed-bean-class>com.idega.manager.bean.ModuleManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>InstallManager</managed-bean-name>
		<managed-bean-class>com.idega.manager.bean.InstallManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>InstallListManager</managed-bean-name>
		<managed-bean-class>com.idega.manager.bean.InstallListManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>InstallNewModuleListManager</managed-bean-name>
		<managed-bean-class>com.idega.manager.bean.InstallNewModuleListManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
  		<from-view-id>/workspace/manager/install/</from-view-id>
  		<navigation-case>
    	<from-outcome>next</from-outcome>
    	<to-view-id>/workspace/manager/install/installUpdate/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>cancel</from-outcome>
    	<to-view-id>/workspace/manager/</to-view-id>
  		</navigation-case>
	</navigation-rule>
	<navigation-rule>
  		<from-view-id>/workspace/manager/install/installUpdate/</from-view-id>
  		<navigation-case>
    	<from-outcome>updateModules</from-outcome>
    	<to-view-id>/workspace/manager/install/update/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>installNewModules</from-outcome>
    	<to-view-id>/workspace/manager/install/newModule/</to-view-id>
  		</navigation-case>
 		<navigation-case>
    	<from-outcome>cancel</from-outcome>
    	<to-view-id>/workspace/manager/</to-view-id>
  		</navigation-case>
	</navigation-rule>
	<navigation-rule>
  		<from-view-id>/workspace/manager/install/update/</from-view-id>
  		<navigation-case>
    	<from-outcome>next</from-outcome>
    	<to-view-id>/workspace/manager/install/commit/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>back</from-outcome>
    	<to-view-id>/workspace/manager/install/installUpdate/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>cancel</from-outcome>
    	<to-view-id>/workspace/manager/</to-view-id>
  		</navigation-case>
	</navigation-rule>
	<navigation-rule>
  		<from-view-id>/workspace/manager/install/commit/</from-view-id>
  		<navigation-case>
    	<from-outcome>backUpdate</from-outcome>
    	<to-view-id>/workspace/manager/install/update/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>backInstall</from-outcome>
    	<to-view-id>/workspace/manager/install/newModuleVersion/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>cancel</from-outcome>
    	<to-view-id>/workspace/manager/</to-view-id>
  		</navigation-case>
	</navigation-rule>
	<navigation-rule>
  		<from-view-id>/workspace/manager/install/newModule/</from-view-id>
  		<navigation-case>
    	<from-outcome>next</from-outcome>
    	<to-view-id>/workspace/manager/install/newModuleVersion/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>back</from-outcome>
    	<to-view-id>/workspace/manager/install/installUpdate/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>cancel</from-outcome>
    	<to-view-id>/workspace/manager/</to-view-id>
  		</navigation-case>
	</navigation-rule>
	<navigation-rule>
  		<from-view-id>/workspace/manager/install/newModuleVersion/</from-view-id>
  		<navigation-case>
    	<from-outcome>next</from-outcome>
    	<to-view-id>/workspace/manager/install/commit/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>back</from-outcome>
    	<to-view-id>/workspace/manager/install/newModule/</to-view-id>
  		</navigation-case>
  		<navigation-case>
    	<from-outcome>cancel</from-outcome>
    	<to-view-id>/workspace/manager/</to-view-id>
  		</navigation-case>
	</navigation-rule>
	
	<managed-bean>
		<managed-bean-name>SystemSettings</managed-bean-name>
		<managed-bean-class>com.idega.manager.bean.SystemSettings</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>CacheSettings</managed-bean-name>
		<managed-bean-class>com.idega.manager.bean.CacheSettings</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

</faces-config>
